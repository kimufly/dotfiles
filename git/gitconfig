[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[core]
	editor = nvim
[alias]
    ci = commit -a -v 
    st = status -s 
    br = branch 
    cl = clone
    pl = pull
    ps = push
    throw = reset --hard HEAD
    throwh = reset --hard HEAD^
